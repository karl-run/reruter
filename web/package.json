{
  "name": "reruter-web",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-nano-spinner": "^1.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.1",
    "serve": "^6.5.3",
    "styled-components": "^3.2.5",
    "styled-reset": "^1.3.2"
  },
  "scripts": {
    "build": "react-scripts build",
    "serve": "serve --single ./build",
    "start": "yarn serve",
    "start:dev": "react-scripts start",
    "test": "react-scripts test --env=jsdom",
    "deploy": "now && now alias && now rm reruter-web --safe --yes",
    "deploy:ci": "now --token $NOW_TOKEN && now alias --token $NOW_TOKEN && now rm reruter-web --token $NOW_TOKEN --safe --yes"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 120
  }
}
